    "browser file",
    "file system access"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taotao7/file-tree"
  },
  "author": "taotao7",
  "license": "ISC",
  "packageManager": "pnpm@9.6.0",
  "devDependencies": {
    "@types/wicg-file-system-access": "^2023.10.5",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
